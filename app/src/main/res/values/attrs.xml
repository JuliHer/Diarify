        <attr name="backgroundBorder" format="color" />
        <attr name="subTextColor" format="color" />

        <attr name="daysInToolbar" format="color" />
        <attr name="daysWeekend" format="color" />
        <attr name="daysTextHighlight" format="color" />
        <attr name="daysHighlight" format="color" />
        <attr name="hourText" format="color" />
        <attr name="iconHint" format="color" />
        <attr name="backgroundHint" format="color" />
        <attr name="iMainColor" format="color" />
        <attr name="borderLighting" format="color" />
        <attr name="backgroundLighting" format="color" />

        <attr name="palette_blue" format="color" />
        <attr name="palette_green" format="color" />
        <attr name="palette_yellow" format="color" />
        <attr name="palette_red" format="color" />
