        <attr name="shimmerSkeleton" format="color" />
        <attr name="maskSkeleton" format="color" />
        <attr name="shimmerHintSkeleton" format="color" />
        <attr name="maskHintSkeleton" format="color" />

        <attr name="highInputColor" format="color" />
        <attr name="hintInputColor" format="color" />
        <attr name="hintViewGrayColor" format="color" />
        <attr name="colorThird" format="color" />
        <attr name="MainColor" format="color" />
        <attr name="backgroundChat" format="reference" />
    </declare-styleable>

    <declare-styleable name="CalendarWeekView">
        <attr name="android:clipToPadding" />
        <attr name="weekPaddingBottom" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="textEventSize" format="dimension" />
        <attr name="backgroundColor" format="color" />
        <attr name="gridColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="textHourColor" format="color" />
        <attr name="textDayColor" format="color" />
        <attr name="textDayWeekendColor" format="color" />
        <attr name="highLightColor" format="color" />
        <attr name="textHighLightColor" format="color" />
    </declare-styleable>

    <declare-styleable name="Calendar">
        <attr name="ColorDays" format="color" />
        <attr name="ColorDaysInGrid" format="color" />
        <attr name="ColorWeekendDays" format="color" />
        <attr name="ColorWeekendDaysInGrid" format="color" />
        <attr name="ColorToday" format="color" />
        <attr name="ColorTextHighlight" format="color" />
        <attr name="ColorHighlight" format="color" />
        <attr name="ColorMonth" format="color" />
        <attr name="ColorOutMonth" format="color" />
    </declare-styleable>

    <declare-styleable name="LineChart">
        <attr name="hintColors" format="color" />
        <attr name="textValuesColor" format="color" />
        <attr name="textTitleColor" format="color" />
        <attr name="textValuesSize" format="dimension" />
        <attr name="textTitleSize" format="dimension" />
        <attr name="padding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MsgFlexboxLayout">
        <attr name="viewMain" format="reference" />
        <attr name="viewSlave" format="reference" />
        <attr name="maxWidth" format="dimension" />
    </declare-styleable>
    <declare-styleable name="AspectRatioView">
        <attr name="aspectRatio" format="string" />
        <attr name="orientationRatio" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ScaleAndDropLayout">
        <attr name="paddingCrop" format="dimension" />
        <attr name="maxCropperSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="AppWidgetAttrs">
        <attr name="appWidgetPadding" format="dimension" />
        <attr name="appWidgetInnerRadius" format="dimension" />
        <attr name="appWidgetRadius" format="dimension" />
    </declare-styleable>
</resources>